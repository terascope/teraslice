{
    "name": "example-data-generator-job",
    "lifecycle": "once",
    "workers": 2,
    "assets": [
        "example"
    ],
    "volumes": [
        {
            "name": "task-pv-claim",
            "path": "/srv"
        }
    ],
    "operations": [
        {
            "_op": "elasticsearch_data_generator",
            "size": 5000000
        },
        {
            "_op": "example-op"
        },
        {
            "_op": "elasticsearch_index_selector",
            "index": "terak8s-example-data",
            "type": "events"
        },
        {
            "_op": "elasticsearch_bulk",
            "size": 5000
        }
    ]
}
