{
    "name": "example-data-generator-job-with-override",
    "lifecycle": "once",
    "workers": 2,
    "assets": [
        "example-asset",
        "elasticsearch",
        "standard"
    ],
    "ephemeral_storage": true,
    "operations": [
        {
            "_op": "data_generator",
            "size": 5000000
        },
        {
            "_op": "example"
        },
        {
            "_op": "delay",
            "ms": 30000
        },
        {
            "_op": "elasticsearch_bulk",
            "index": "terak8s-example-data",
            "type": "events",
            "size": 5000
        }
    ],
    "pod_spec_override": {
        "initContainers": [
            {
                "name": "init-hello-world",
                "image": "busybox:1.28",
                "command": [
                    "sh",
                    "-c",
                    "echo 'HELLO WORLD'"
                ]
            }
        ]
    },
    "__metadata": {
        "cli": {
            "cluster": "http://127.0.0.1:52972",
            "version": "0.55.0",
            "job_id": "38f5151f-6230-4c34-a717-f3db5b268b11",
            "updated": "2023-10-04T16:45:31.404Z"
        }
    }
}
