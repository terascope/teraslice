{
    "name": "example-data-generator-job-targets",
    "lifecycle": "once",
    "workers": 2,
    "assets": [
        "example",
        "elasticsearch"
    ],
    "targets": [
        {
            "key": "zone",
            "value": "west"
        }
    ],
    "operations": [
        {
            "_op": "elasticsearch_data_generator",
            "size": 5000000
        },
        {
            "_op": "example-op"
        },
        {
            "_op": "elasticsearch_bulk",
            "size": 5000,
            "index": "terak8s-example-data",
            "type": "events"
        }
    ]
}
