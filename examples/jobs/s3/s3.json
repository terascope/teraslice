{
    "name": "s3-writer",
    "workers": 1,
    "lifecycle": "once",
    "assets": [
        "elasticsearch",
        "file",
        "standard"
    ],
    "apis": [
        {
            "_name": "elasticsearch_reader_api",
            "index": "example-logs",
            "type": "logs",
            "size": 10000,
            "date_field_name": "created",
            "preserve_id": true
        },
        {
            "_name": "s3_exporter_api",
            "path": "example-logs",
            "format": "ldjson",
            "connection": "minio"
        }
    ],
    "operations": [
        {
            "_op": "elasticsearch_reader",
            "api_name": "elasticsearch_reader_api"
        },
        {
            "_op": "s3_exporter",
            "api_name": "s3_exporter_api"
        }
    ]
}
