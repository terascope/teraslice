{
    "name": "Kafka To ES 3.4.1",
    "lifecycle": "persistent",
    "workers": 1,
    "assets": [
        "elasticsearch:3.4.1",
        "standard:0.14.2",
        "kafka"
    ],
    "operations": [
        {
            "_op": "kafka_reader",
            "connection": "default",
            "topic": "test1",
            "group": "testroll-es-3-4-1",
            "rollback_on_failure": true,
            "size": 10000,
            "wait": 5000
        },
        {
            "_op": "set_key",
            "field": "uuid"
        },
        {
            "_op": "elasticsearch_bulk",
            "index": "testroll-es-3-4-1",
            "type": "_doc",
            "upsert": true,
            "size": 10000,
            "connection": "es_data1"
        }
    ]
}
