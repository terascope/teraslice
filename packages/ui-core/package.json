{
    "name": "@terascope/ui-core",
    "version": "0.4.2",
    "description": "A teraserver ui framework",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "start": "env REACT_APP_DEV_MODE=true craco start",
        "build": "craco build",
        "build:watch": "env FORCE_COLOR=1 npm-watch build",
        "prepublishOnly": "yarn build",
        "test": "env CI=true FORCE_COLOR=1 REACT_APP_DEV_MODE=true craco test --colors",
        "test:watch": "env REACT_APP_DEV_MODE=true craco test",
        "eject": "craco eject"
    },
    "watch": {
        "build": {
            "patterns": [
                "./src",
                "../ui-components/dist/src",
                "../ui-components/index.css"
            ],
            "delay": 500,
            "ignore": "build",
            "extensions": ".css,.js,.jsx,.tsx,.ts",
            "quiet": false
        }
    },
    "files": [
        "build/**/*",
        "index.js"
    ],
    "homepage": "/v2/ui",
    "repository": "git@github.com:terascope/teraslice.git",
    "author": "Terascope, LLC <info@terascope.io>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "main": "index.js",
    "dependencies": {
        "@terascope/ui-data-access": "^0.3.0",
        "pkg-up": "^3.1.0"
    },
    "devDependencies": {
        "@craco/craco": "^5.2.3",
        "@terascope/ui-components": "^0.3.2",
        "@terascope/utils": "^0.11.1",
        "@types/jest": "24.0.15",
        "@types/node": "12.0.10",
        "@types/react": "16.8.22",
        "@types/react-dom": "16.8.4",
        "@types/react-router-dom": "^4.3.4",
        "apollo-boost": "^0.4.3",
        "apollo-client": "^2.6.3",
        "graphql": "^14.3.1",
        "history": "^4.9.0",
        "npm-watch": "^0.6.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.8.1",
        "react": "^16.8.6",
        "react-apollo": "^2.5.7",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.87.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
