{
    "name": "@terascope/teraslice-messaging",
    "displayName": "Teraslice Messaging",
    "version": "1.13.1",
    "description": "An internal teraslice messaging library using socket.io",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-messaging#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "files": [
        "dist/src/**/*"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "yarn build --watch",
        "test": "node ../scripts/bin/ts-scripts test ../teraslice-messaging --",
        "test:debug": "node ../scripts/bin/ts-scripts test --debug ../teraslice-messaging --",
        "test:watch": "node ../scripts/bin/ts-scripts test --watch ../teraslice-messaging --"
    },
    "resolutions": {
        "debug": "~4.4.1",
        "ms": "~2.1.3"
    },
    "dependencies": {
        "@terascope/types": "~1.4.4",
        "@terascope/utils": "~1.10.1",
        "get-port": "~7.1.0",
        "ms": "~2.1.3",
        "nanoid": "~5.1.5",
        "p-event": "~6.0.1",
        "socket.io": "~1.7.4",
        "socket.io-client": "~1.7.4"
    },
    "devDependencies": {
        "@types/ms": "~0.7.34",
        "@types/socket.io": "~2.1.13",
        "@types/socket.io-client": "~1.4.36"
    },
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit"
    }
}
