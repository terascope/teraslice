{
  "name": "@terascope/teraslice-op-test-harness",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A testing harness to simplify testing Teraslice processors and operations.",
  "main": "index.js",
  "scripts": {
    "docs": "jsdoc -P package.json index.js",
    "lint": "eslint *.js spec",
    "lint:fix": "eslint --fix *.js spec",
    "test": "nyc -x spec/ --reporter=text-summary jasmine && nyc report --reporter=html",
    "report-coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov && codecov"
  },
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "*.js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ]
  },
  "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-op-test-harness#readme",
  "repository": "git@github.com:terascope/teraslice.git",
  "author": "Terascope, LLC <info@terascope.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/terascope/teraslice/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "teraslice": "^0.37.1"
  },
  "devDependencies": {
    "codecov": "^3.0.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.3.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jsdoc": "^3.4.3",
    "nyc": "^12.0.2"
  }
}
