        "@terascope/utils": "~1.4.2",
        "@types/validator": "~13.12.2",
        "awesome-phonenumber": "~7.2.0",
        "date-fns": "~4.1.0",
        "ip-bigint": "~8.2.0",
        "ip6addr": "~0.2.5",
        "ipaddr.js": "~2.2.0",
        "is-cidr": "~5.1.0",
        "jexl": "~2.3.0",
        "lodash": "~4.17.21",
        "mnemonist": "~0.39.8",
        "uuid": "~11.0.3",
        "valid-url": "~1.0.9",
        "validator": "~13.12.0",
        "xlucene-parser": "~1.4.3"
