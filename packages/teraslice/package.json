{
    "name": "teraslice",
    "version": "0.50.0",
    "description": "Distributed computing platform for processing JSON data",
    "bin": "service.js",
    "main": "index.js",
    "scripts": {
        "start": "node service.js",
        "lint": "eslint *.js lib/**/*.js test/**/*.js",
        "lint:fix": "yarn lint --fix",
        "test": "jest",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
        "test:debug": "env DEBUG=\"${DEBUG:-*teraslice*,-teraslice-messaging*}\" jest --colors --forceExit --detectOpenHandles --coverage=false --runInBand"
    },
    "directories": {
        "lib": "lib",
        "test": "test",
        "bin": "service.js",
        "main": "index.js"
    },
    "files": [
        "*.js",
        "lib/**/*",
        "examples/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "author": "Terascope, LLC <info@terascope.io>",
    "homepage": "https://github.com/terascope/teraslice#readme",
    "dependencies": {
        "@terascope/elasticsearch-api": "^2.0.1",
        "@terascope/error-parser": "^1.0.2",
        "@terascope/job-components": "^0.15.0",
        "@terascope/queue": "^1.1.6",
        "@terascope/teraslice-messaging": "^0.3.1",
        "@terascope/utils": "^0.6.0",
        "async-mutex": "^0.1.3",
        "barbe": "^3.0.15",
        "bluebird": "^3.5.3",
        "body-parser": "^1.18.2",
        "convict": "^4.4.1",
        "decompress": "^4.2.0",
        "easy-table": "^1.1.1",
        "express": "^4.16.4",
        "fs-extra": "^7.0.1",
        "js-yaml": "^3.12.2",
        "kubernetes-client": "^6.10.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "nanoid": "^2.0.1",
        "p-whilst": "^1.0.0",
        "porty": "^3.1.1",
        "request": "^2.88.0",
        "shortid": "^2.2.14",
        "socket.io": "^1.7.4",
        "socket.io-client": "^1.7.4",
        "terafoundation": "^0.8.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@terascope/teraslice-op-test-harness": "^1.5.4",
        "archiver": "^3.0.0",
        "bufferstreams": "^2.0.1",
        "chance": "^1.0.18",
        "elasticsearch": "^15.4.1",
        "got": "^9.6.0",
        "jest-fixtures": "^0.6.0",
        "nock": "^10.0.6"
    }
}
