{
  "name": "teraslice",
  "version": "0.38.1",
  "description": "Slice and dice your Elasticsearch data",
  "bin": "service.js",
  "main": "index.js",
  "scripts": {
    "start": "node service.js",
    "lint": "eslint *.js lib test",
    "lint:fix": "eslint --fix *.js lib test",
    "test": "jest --forceExit",
    "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand"
  },
  "directories": {
    "example": "examples",
    "doc": "docs",
    "lib": "lib",
    "test": "test",
    "bin": "service.js",
    "main": "index.js"
  },
  "files": [
    "*.js",
    "lib/**/*",
    "examples/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terascope/teraslice.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/terascope/teraslice/issues"
  },
  "author": "Terascope, LLC <info@terascope.io>",
  "homepage": "https://github.com/terascope/teraslice#readme",
  "dependencies": {
    "@terascope/elasticsearch-api": "^1.0.0",
    "@terascope/error-parser": "^1.0.0",
    "@terascope/queue": "^1.1.3",
    "@terascope/teraslice-operations": "^0.1.0",
    "@terascope/teraslice-validators": "^0.1.0",
    "barbe": "^3.0.14",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "convict": "^4.3.2",
    "datemath-parser": "^1.0.6",
    "decompress": "^4.2.0",
    "easy-table": "^1.1.1",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "kubernetes-client": "^5.3.0",
    "lodash": "^4.17.5",
    "mocker-data-generator": "^2.6.1",
    "moment": "^2.21.0",
    "node-cache": "^4.2.0",
    "request": "^2.88.0",
    "shortid": "^2.2.12",
    "socket.io": "~1.4.5",
    "socket.io-client": "~1.4.5",
    "terafoundation": "^0.2.1",
    "uuid": "^3.3.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@terascope/teraslice-op-test-harness": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.5.0",
    "jest-extended": "^0.8.1",
    "json-schema-faker": "^0.5.0-rc13",
    "nock": "^9.6.1"
  }
}
