{
    "name": "@terascope/job-components",
    "version": "0.7.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Teraslice Job Components",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/job-components#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "directories": {
        "lib": "dist",
        "test": "test"
    },
    "files": [
        "dist/**/*"
    ],
    "srcMain": "src/index.ts",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "tslint --project tsconfig.json --format verbose",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "build": "tsc --project tsconfig.build.json --pretty",
        "build:prod": "tsc --project tsconfig.build.json",
        "build:watch": "yarn build --watch",
        "test": "jest",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
        "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand",
        "benchmark": "yarn build; node bench/index.js"
    },
    "dependencies": {
        "@terascope/queue": "^1.1.4",
        "convict": "^4.4.0",
        "datemath-parser": "^1.0.6",
        "debug": "^4.1.0",
        "fs-extra": "^7.0.0",
        "lodash.clonedeep": "^4.5.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/jest": "^23.3.8",
        "@types/node": "^10.12.0",
        "babel-core": "^6.0.0",
        "babel-jest": "^23.6.0",
        "benchmark": "^2.1.4",
        "jest": "^23.6.0",
        "jest-extended": "^0.11.0",
        "ts-jest": "^23.10.4",
        "tslint": "^5.0.0",
        "tslint-config-airbnb": "^5.11.0",
        "typescript": "^3.1.3"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
