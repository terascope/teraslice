        "fs-extra": "^9.1.0",
        "jest-fixtures": "^0.6.0"
    },
    "engines": {
        "node": "^12.20.0 || >=14.17.0",
        "yarn": ">=1.16.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit-a"
    }
