{
    "name": "@terascope/job-components",
    "version": "0.5.1",
    "publishConfig": {
        "access": "public"
    },
    "description": "Teraslice Job Components",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/job-components#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "directories": {
        "lib": "dist",
        "test": "test"
    },
    "files": [
        "dist/**/*"
    ],
    "srcMain": "src/index.ts",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "tslint --project tsconfig.json --format verbose",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "build": "rimraf dist && tsc --project tsconfig.build.json --pretty",
        "build:prod": "tsc --project tsconfig.build.json",
        "build:watch": "yarn build --watch",
        "test": "jest",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
        "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand"
    },
    "dependencies": {
        "@terascope/queue": "^1.1.4",
        "@terascope/teraslice-types": "^0.2.1",
        "@types/fs-extra": "^5.0.4",
        "@types/lodash": "^4.14.117",
        "@types/node": "^10.12.0",
        "@types/uuid": "^3.4.4",
        "datemath-parser": "^1.0.6",
        "fs-extra": "^7.0.0",
        "list": "^2.0.15",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/jest": "^23.3.5",
        "babel-core": "^6.0.0",
        "babel-jest": "^23.6.0",
        "jest": "^23.6.0",
        "jest-extended": "^0.11.0",
        "rimraf": "^2.0.0",
        "ts-jest": "^23.10.4",
        "tslint": "^5.0.0",
        "tslint-config-airbnb": "^5.11.0",
        "typescript": "^3.1.3"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
