{
    "name": "teraslice-test-harness",
    "version": "0.6.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "A helpful library for testing teraslice jobs, operations, and other components.",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-test-harness#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "author": "Terascope, LLC <info@terascope.io>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "files": [
        "dist/src/**/*"
    ],
    "srcMain": "src/index.ts",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "saveDev": true,
    "scripts": {
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "build": "tsc --build --pretty",
        "build:watch": "yarn build --watch",
        "test": "jest --forceExit",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
        "test:debug": "env DEBUG=\"${DEBUG:-*teraslice*}\" jest --detectOpenHandles --coverage=false --runInBand"
    },
    "dependencies": {
        "@terascope/job-components": "^0.18.0",
        "@terascope/teraslice-op-test-harness": "^1.6.0",
        "lodash": "^4.17.11"
    },
    "devDependencies": {},
    "engines": {
        "node": ">=8.0.0"
    }
}
