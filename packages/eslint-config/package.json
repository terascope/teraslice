{
    "name": "@terascope/eslint-config",
    "displayName": "Terascope ESLint Config",
    "version": "1.1.4",
    "description": "A shared eslint config based on eslint-config-airbnb",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "yarn workspace @terascope/scripts ts-scripts test ../eslint-config --",
        "test:debug": "yarn workspace @terascope/scripts ts-scripts test --debug ../eslint-config --",
        "test:watch": "yarn workspace @terascope/scripts ts-scripts test --watch ../eslint-config --"
    },
    "dependencies": {
        "@eslint/compat": "~1.2.5",
        "@eslint/js": "~9.18.0",
        "@stylistic/eslint-plugin": "~2.13.0",
        "@types/eslint__js": "~8.42.3",
        "@typescript-eslint/eslint-plugin": "~8.21.0",
        "@typescript-eslint/parser": "~8.21.0",
        "eslint": "~9.18.0",
        "eslint-plugin-import": "~2.31.0",
        "eslint-plugin-jest": "~28.11.0",
        "eslint-plugin-jest-dom": "~5.5.0",
        "eslint-plugin-jsx-a11y": "~6.10.2",
        "eslint-plugin-react": "~7.37.4",
        "eslint-plugin-react-hooks": "~5.1.0",
        "eslint-plugin-testing-library": "~7.1.1",
        "globals": "~15.14.0",
        "typescript": "~5.7.3",
        "typescript-eslint": "~8.21.0"
    },
    "engines": {
        "node": ">=18.18.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "terascope": {
        "enableTypedoc": false,
        "testSuite": "unit"
    }
}
