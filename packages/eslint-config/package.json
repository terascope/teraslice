{
    "name": "@terascope/eslint-config",
    "displayName": "Terascope ESLint Config",
    "version": "1.1.19",
    "description": "A shared eslint config based on eslint-config-airbnb",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "node ../scripts/bin/ts-scripts test ../eslint-config --",
        "test:debug": "node ../scripts/bin/ts-scripts test --debug ../eslint-config --",
        "test:watch": "node ../scripts/bin/ts-scripts test --watch ../eslint-config --"
    },
    "dependencies": {
        "@eslint/compat": "~1.3.1",
        "@eslint/js": "~9.30.1",
        "@stylistic/eslint-plugin": "~5.1.0",
        "@typescript-eslint/eslint-plugin": "~8.35.1",
        "@typescript-eslint/parser": "~8.35.1",
        "eslint": "~9.30.0",
        "eslint-plugin-import": "~2.32.0",
        "eslint-plugin-jest": "~29.0.1",
        "eslint-plugin-jest-dom": "~5.5.0",
        "eslint-plugin-jsx-a11y": "~6.10.2",
        "eslint-plugin-react": "~7.37.5",
        "eslint-plugin-react-hooks": "~5.2.0",
        "eslint-plugin-testing-library": "~7.5.3",
        "globals": "~16.2.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "~8.35.1"
    },
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "terascope": {
        "enableTypedoc": false,
        "testSuite": "unit"
    }
}
