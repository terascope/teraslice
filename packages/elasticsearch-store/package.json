    },
    "dependencies": {
        "@opensearch-project/opensearch": "^1.0.2",
        "@terascope/data-mate": "^0.38.0",
        "@terascope/data-types": "^0.35.0",
        "@terascope/types": "^0.10.2",
        "@terascope/utils": "^0.44.1",
        "ajv": "^6.12.6",
        "elasticsearch6": "npm:@elastic/elasticsearch@^6.7.0",
        "elasticsearch7": "npm:@elastic/elasticsearch@^7.0.0",
        "elasticsearch8": "npm:@elastic/elasticsearch@^8.0.0",
        "uuid": "^8.3.2",
        "xlucene-translator": "^0.27.0"
    },
    "devDependencies": {
        "@types/uuid": "^8.3.4"
    },
    "engines": {
        "node": "^12.22.0 || >=14.17.0",
        "yarn": ">=1.16.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "elasticsearch"
