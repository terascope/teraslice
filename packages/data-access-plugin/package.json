{
    "name": "@terascope/data-access-plugin",
    "version": "0.12.3",
    "description": "A teraserver plugin for managing data access and searching spaces",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/data-access-plugin#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "files": [
        "dist/src/**/*"
    ],
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "bin": {
        "create-superadmin": "./bin/create-superadmin.js",
        "simple-migrate": "./bin/simple-migrate.js"
    },
    "scripts": {
        "build": "tsc --build --pretty",
        "build:watch": "yarn build --watch",
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "test": "ts-scripts test . --",
        "test:debug": "ts-scripts test --debug . --",
        "test:watch": "ts-scripts test --watch . --"
    },
    "dependencies": {
        "@apollographql/graphql-playground-html": "^1.6.24",
        "@terascope/data-access": "^0.13.2",
        "@terascope/data-types": "^0.5.3",
        "@terascope/elasticsearch-api": "^2.1.4",
        "@terascope/utils": "^0.16.0",
        "accepts": "^1.3.7",
        "apollo-server-express": "~2.6.5",
        "body-parser": "^1.19.0",
        "elasticsearch-store": "^0.11.1",
        "graphql": "^14.3.1",
        "graphql-iso-date": "^3.6.1",
        "graphql-type-json": "^0.3.0",
        "terafoundation": "^0.11.2",
        "xlucene-evaluator": "^0.9.7"
    },
    "devDependencies": {
        "@terascope/job-components": "^0.20.8",
        "@types/express": "^4.17.0",
        "@types/got": "^9.6.6",
        "@types/graphql-iso-date": "^3.3.1",
        "@types/graphql-type-json": "^0.3.0",
        "elasticsearch": "^15.4.1",
        "express": "^4.17.1",
        "got": "^9.6.0",
        "graphql-request": "^1.8.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "displayName": "Data Access Plugin",
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": false,
        "testSuite": "elasticsearch"
    }
}
