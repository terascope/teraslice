        "test:watch": "node ../scripts/bin/ts-scripts test --watch ../geo-utils --"
    },
    "dependencies": {
        "@terascope/types": "~2.0.0-dev.5",
        "@terascope/core-utils": "~2.0.0-dev.5",
        "mnemonist": "~0.40.3"
    },
    "devDependencies": {},
    "engines": {
        "node": ">=22.0.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit"
    }
}
