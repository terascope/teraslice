{
    "name": "ts-transforms",
    "version": "0.20.0",
    "description": "An ETL framework built upon xlucene-evaluator",
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/ts-transforms#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "author": "Terascope, LLC <info@terascope.io>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "srcMain": "src/index.ts",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "files": [
        "bin/**/*",
        "dist/src/**/*"
    ],
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "bin": {
        "ts-transform": "./bin/ts-transform.js",
        "ts-match": "./bin/ts-transform.js"
    },
    "scripts": {
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "build": "tsc --build --pretty",
        "build:watch": "yarn build --watch",
        "test": "jest --forceExit",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
        "test:debug": "env DEBUG=\"${DEBUG:-*teraslice*}\" jest --detectOpenHandles --coverage=false --runInBand"
    },
    "dependencies": {
        "@terascope/utils": "^0.10.2",
        "@types/graphlib": "^2.1.5",
        "@types/shortid": "^0.0.29",
        "awesome-phonenumber": "^2.10.0",
        "graphlib": "^2.1.7",
        "lodash": "^4.17.11",
        "shortid": "^2.2.14",
        "valid-url": "^1.0.9",
        "validator": "^10.10.0",
        "xlucene-evaluator": "^0.8.1",
        "yargs": "^13.2.4"
    },
    "devDependencies": {
        "@types/valid-url": "^1.0.2",
        "@types/validator": "^10.11.0",
        "@types/yargs": "^13.0.0"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
