{
    "name": "xlucene-parser",
    "displayName": "xLucene Parser",
    "version": "2.2.0",
    "description": "Flexible Lucene-like evaluator and language parser",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/xlucene-parser#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "sideEffects": false,
    "type": "module",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "files": [
        "dist/src/**/*"
    ],
    "scripts": {
        "benchmark": "pnpm build && node bench/index.js",
        "prebuild": "./scripts/generate-engine.js",
        "build": "pnpm prebuild && tsc --build",
        "build:watch": "pnpm build --watch",
        "test": "node ../scripts/bin/ts-scripts test ../xlucene-parser --",
        "test:debug": "node ../scripts/bin/ts-scripts test --debug ../xlucene-parser --",
        "test:watch": "node ../scripts/bin/ts-scripts test --watch ../xlucene-parser --"
    },
    "dependencies": {
        "@terascope/core-utils": "workspace:*",
        "@terascope/geo-utils": "workspace:*",
        "@terascope/ip-utils": "workspace:*",
        "@terascope/types": "workspace:*",
        "peggy": "~5.0.6",
        "ts-pegjs": "~4.2.1"
    },
    "devDependencies": {
        "@turf/invariant": "~7.3.3",
        "@turf/random": "~7.3.3",
        "date-fns": "~4.1.0"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=10.25.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit"
    }
}
