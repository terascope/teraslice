{
    "name": "teraslice-cli",
    "displayName": "Teraslice CLI",
    "version": "0.20.1",
    "description": "Command line manager for teraslice jobs, assets, and cluster references.",
    "keywords": [
        "teraslice"
    ],
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-cli#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "main": "dist/src/command.js",
    "typings": "dist/src/index.d.ts",
    "bin": {
        "earl": "bin/teraslice-cli.js",
        "teraslice-cli": "bin/teraslice-cli.js"
    },
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "files": [
        "bin/**/*",
        "generator-templates/**/*",
        "dist/src/**/*"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "yarn build --watch",
        "test": "ts-scripts test . --",
        "test:debug": "ts-scripts test --debug . --",
        "test:watch": "ts-scripts test --watch . --"
    },
    "dependencies": {
        "@terascope/fetch-github-release": "^0.6.0",
        "@terascope/utils": "^0.26.1",
        "archiver": "^3.0.0",
        "chalk": "^3.0.0",
        "cli-table3": "^0.6.0",
        "easy-table": "^1.1.1",
        "fs-extra": "^9.0.0",
        "js-yaml": "^3.13.1",
        "node-yaml": "^4.0.0",
        "prompts": "^2.3.2",
        "request": "^2.88.0",
        "request-promise": "^4.2.5",
        "signale": "^1.4.0",
        "teraslice-client-js": "^0.18.1",
        "tmp": "^0.1.0",
        "tty-table": "^4.1.1",
        "yargs": "^15.3.1",
        "yeoman-environment": "^2.7.0",
        "yeoman-generator": "^4.7.0",
        "yo": "^3.1.1"
    },
    "devDependencies": {
        "@types/archiver": "^3.0.0",
        "@types/decompress": "^4.2.3",
        "@types/easy-table": "^0.0.32",
        "@types/prompts": "^2.0.5",
        "@types/signale": "^1.4.0",
        "@types/tmp": "^0.1.0",
        "@types/yargs": "^15.0.1",
        "@types/yeoman-assert": "^3.1.1",
        "@types/yeoman-environment": "^2.3.3",
        "@types/yeoman-generator": "^3.1.4",
        "decompress": "^4.2.0",
        "jest-fixtures": "^0.6.0",
        "nock": "^12.0.3",
        "yeoman-assert": "^3.1.1",
        "yeoman-test": "^2.4.1"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "pkg": {
        "scripts": "cmds/**/*.js",
        "assets": [
            "package.json",
            "generators-templates/**/*"
        ]
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "testSuite": "unit-a",
        "enableTypedoc": true
    }
}
