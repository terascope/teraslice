{
    "name": "teraslice-cli",
    "displayName": "Teraslice CLI",
    "version": "1.6.1",
    "description": "Command line manager for teraslice jobs, assets, and cluster references.",
    "keywords": [
        "teraslice"
    ],
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/teraslice-cli#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "type": "module",
    "main": "dist/src/command.js",
    "typings": "dist/src/index.d.ts",
    "bin": {
        "earl": "bin/teraslice-cli.js",
        "teraslice-cli": "bin/teraslice-cli.js"
    },
    "directories": {
        "lib": "dist/src",
        "test": "test"
    },
    "files": [
        "bin/**/*",
        "generator-templates/**/*",
        "dist/src/**/*"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "yarn build --watch",
        "test": "NODE_OPTIONS='--experimental-vm-modules' ts-scripts test . --",
        "test:debug": "NODE_OPTIONS='--experimental-vm-modules' ts-scripts test --debug . --",
        "test:watch": "NODE_OPTIONS='--experimental-vm-modules' ts-scripts test --watch . --"
    },
    "dependencies": {
        "@terascope/fetch-github-release": "^0.8.10",
        "@terascope/types": "^0.17.2",
        "@terascope/utils": "^0.59.2",
        "chalk": "^4.1.2",
        "cli-table3": "^0.6.4",
        "easy-table": "^1.2.0",
        "ejs": "^3.1.10",
        "esbuild": "^0.21.3",
        "events": "^3.3.0",
        "execa": "^5.1.0",
        "fs-extra": "^11.2.0",
        "glob": "^8.0.3",
        "glob-promise": "5.0.0",
        "js-yaml": "^4.1.0",
        "pretty-bytes": "^5.6.0",
        "prompts": "^2.4.2",
        "signale": "^1.4.0",
        "teraslice-client-js": "^0.60.2",
        "tmp": "^0.2.0",
        "tty-table": "^4.2.3",
        "yargs": "^17.7.2",
        "yo": "^4.3.1"
    },
    "devDependencies": {
        "@types/decompress": "^4.2.7",
        "@types/easy-table": "^1.2.0",
        "@types/ejs": "^3.1.5",
        "@types/js-yaml": "^4.0.9",
        "@types/prompts": "^2.4.9",
        "@types/signale": "^1.4.7",
        "@types/tmp": "^0.2.6",
        "@types/yargs": "^17.0.32",
        "decompress": "^4.2.1",
        "jest-fixtures": "^0.6.0",
        "nock": "^13.5.1"
    },
    "engines": {
        "node": ">=14.17.0",
        "yarn": ">=1.16.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "pkg": {
        "scripts": "cmds/**/*.js",
        "assets": [
            "package.json",
            "generators-templates/**/*"
        ]
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "testSuite": "unit-esm",
        "enableTypedoc": true
    }
}
