{
  "name": "terafoundation",
  "version": "0.3.0",
  "scripts": {
    "lint": "eslint *.js lib",
    "lint:fix": "eslint --fix *.js lib",
    "test": "nyc -x spec/ --reporter=text-summary jasmine && nyc report --reporter=html",
    "report-coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov && codecov"
  },
  "files": [
    "*.js",
    "lib/**/*",
    "examples/**/*"
  ],
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "*.js",
      "lib/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ]
  },
  "license": "Apache-2.0",
  "dependencies": {
    "agentkeepalive": "^3.4.1",
    "bluebird": "^3.5.2",
    "bunyan": "^1.8.12",
    "bunyan-elasticsearch": "1.0.1",
    "convict": "^4.3.2",
    "elasticsearch": "^14.2.1",
    "js-yaml": "^3.11.0",
    "lodash": "~4.17.11",
    "moment": "^2.21.0",
    "mongoose": "~4.13.12",
    "node-statsd": "0.1.1",
    "node-webhdfs": "^0.2.1",
    "redis": "^2.8.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^13.0.1"
  }
}
