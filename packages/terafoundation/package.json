        "convict": "^4.4.1",
        "elasticsearch": "^15.4.1",
        "js-yaml": "^4.0.0",
        "mongoose": "~5.11.12",
        "nanoid": "^3.1.21",
        "node-statsd": "0.1.1",
        "node-webhdfs": "^1.0.2",
        "redis": "^3.0.2",
        "yargs": "^16.2.0"
    },
    "devDependencies": {
        "@types/bunyan": "^1.8.6",
        "@types/elasticsearch": "^5.0.37",
        "@types/js-yaml": "^4.0.0",
        "@types/mongoose": "^5.10.3",
        "@types/redis": "^2.8.25"
