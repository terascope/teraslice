        "@types/lodash": "^4.14.191",
        "@types/yargs": "^17.0.22"
    },
    "engines": {
        "node": ">=14.17.0",
        "yarn": ">=1.16.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
