{
    "name": "@terascope/data-types",
    "displayName": "Data Types",
    "version": "1.7.8",
    "description": "A library for defining the data structures and mapping",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/data-types#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "sideEffects": false,
    "type": "module",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "bin": "./bin/data-types.js",
    "files": [
        "dist/src/**/*",
        "bin/*"
    ],
    "scripts": {
        "build": "tsc --build",
        "build:watch": "yarn build --watch",
        "test": "yarn workspace @terascope/scripts ts-scripts test ../data-types --",
        "test:debug": "yarn workspace @terascope/scripts ts-scripts test --debug ../data-types --",
        "test:watch": "yarn workspace @terascope/scripts ts-scripts test --watch ../data-types --"
    },
    "dependencies": {
        "@terascope/types": "~1.4.1",
        "@terascope/utils": "~1.7.8",
        "graphql": "~16.10.0",
        "yargs": "~17.7.2"
    },
    "devDependencies": {
        "@types/yargs": "~17.0.33"
    },
    "engines": {
        "node": ">=18.18.0",
        "yarn": ">=1.22.19"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit"
    }
}
