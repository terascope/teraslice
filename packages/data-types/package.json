        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "@types/lodash.defaultsdeep": "^4.6.6",
        "@types/yargs": "^15.0.1"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true,
        "testSuite": "unit-b"
