{
    "name": "@terascope/data-types",
    "version": "0.3.1",
    "description": "A library for defining the data structures and mapping",
    "homepage": "https://github.com/terascope/teraslice/tree/master/packages/data-types#readme",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "repository": "git@github.com:terascope/teraslice.git",
    "license": "MIT",
    "author": "Terascope, LLC <info@terascope.io>",
    "files": [
        "dist/src/**/*"
    ],
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "bin": {
        "data-types": "./bin/data-types.js"
    },
    "scripts": {
        "build": "tsc --build --pretty",
        "build:watch": "yarn build --watch",
        "lint": "tslint -p tsconfig.json -t verbose -e '**/*.json'",
        "lint:fix": "yarn lint --fix",
        "prepublishOnly": "yarn build",
        "test": "jest",
        "test:debug": "env DEBUG='*teraslice*' jest --detectOpenHandles --coverage=false --runInBand",
        "test:watch": "jest --coverage=false --notify --watch --onlyChanged"
    },
    "dependencies": {
        "@terascope/utils": "^0.12.2",
        "graphql": "^14.3.1",
        "yargs": "^13.2.4"
    },
    "devDependencies": {
        "@types/yargs": "^13.0.0",
        "xlucene-evaluator": "^0.9.4"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "displayName": "Data Types",
    "srcMain": "src/index.ts",
    "terascope": {
        "enableTypedoc": true
    }
}
