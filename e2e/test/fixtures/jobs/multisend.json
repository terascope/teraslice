{
    "name": "Multisend Test",
    "workers": 1,
    "lifecycle": "once",
    "analytics": false,
    "assets": ["elasticsearch"],
    "resources_requests_cpu": 0.1,
    "resources_limits_cpu": 0.5,
    "cpu_execution_controller": 0.2,
    "operations": [
        {
            "_op": "elasticsearch_reader",
            "index": "replace-me-1000",
            "preserve_id": true,
            "date_field_name": "created",
            "type": "change",
            "size": 500
        },
        {
            "_op": "elasticsearch_bulk",
            "index": "multisend-1000",
            "preserve_id": true,
            "type": "change",
            "multisend_index_append": false,
            "size": 100,
            "multisend": true,
            "connection_map": {
                "A": "default",
                "B": "default",
                "*": "default"
            }
        }
    ]
}
