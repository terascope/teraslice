
{
  "name": "ID_Reindex",
  "lifecycle": "once",
  "analytics": true,
  "slicers": 2,
  "workers": 4,
  "assets": ["elasticsearch"],
  "operations": [
    {
      "_op": "id_reader",
      "index": "example-logs-1000",
      "type": "events",
      "size": 500,
      "key_type": "base64url"
    },
    {
      "_op": "elasticsearch_index_selector",
      "index": "test-id_reindex-1000",
      "type": "events"
    },
    {
      "_op": "elasticsearch_bulk",
      "size": 200
    }
  ]
}
