{
    "name": "teraslice-e2e",
    "private": true,
    "version": "0.1.0",
    "description": "Teraslice integration test suite",
    "scripts": {
        "test": "./scripts/run-test.sh",
        "test:ci": "env CI=true ./scripts/run-test.sh",
        "logs": "./scripts/logs.sh",
        "logs:ci": "env CI=true ./scripts/logs.sh",
        "logs-follow": "docker-compose logs --follow --no-color teraslice-worker teraslice-master | awk -F' [|] ' '{print $2}' | bunyan -o short",
        "clean": "docker-compose down --volumes --remove-orphans --timeout=5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/terascope/teraslice.git"
    },
    "keywords": [
        "teraslice",
        "elasticsearch",
        "docker-compose"
    ],
    "author": "Terascope, LLC <info@terascope.io>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "homepage": "https://github.com/terascope/teraslice/tree/master/e2e/#readme",
    "devDependencies": {
        "@terascope/docker-compose-js": "^1.1.2",
        "@terascope/fetch-github-release": "^0.6.0",
        "bluebird": "^3.5.5",
        "bunyan": "^1.8.12",
        "elasticsearch": "^15.4.1",
        "jest": "^24.8.0",
        "jest-extended": "^0.11.1",
        "lodash": "^4.17.11",
        "semver": "^6.1.1",
        "signale": "^1.4.0",
        "teraslice-client-js": "^0.7.1",
        "uuid": "^3.3.2"
    }
}
