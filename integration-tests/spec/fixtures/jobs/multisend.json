{
    "name": "Multisend Test",
    "workers": 1,
    "lifecycle": "once",

    "operations": [
        {
            "_op": "elasticsearch_reader",
            "index": "example-logs-10000",
            "full_response": true,
            "date_field_name": "created",
            "type": "change",
            "size": 500
        },
        {
            "_op": "elasticsearch_index_selector",
            "index": "multisend-10000",
            "preserve_id": true,
            "type": "change"
        },
        {
            "_op": "elasticsearch_bulk",
            "multisend_index_append": false,
            "size": 100,
            "multisend": true,
            "connection_map": {
                "A": "default",
                "B": "default",
                "*": "default"
            }
        }
    ]
}