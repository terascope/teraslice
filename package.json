{
    "name": "teraslice-workspace",
    "displayName": "Teraslice",
    "version": "0.71.1",
    "private": true,
    "homepage": "https://github.com/terascope/teraslice",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "workspaces": [
        "packages/*",
        "e2e"
    ],
    "scripts": {
        "prebuild": "./packages/xlucene-parser/scripts/generate-engine.js",
        "build": "tsc --build",
        "build:cleanup": "./scripts/build-cleanup.sh",
        "build:doctor": "./scripts/build-doctor.sh",
        "build:fix": "echo '[DEPRECATED], use yarn run build:doctor instead'",
        "build:pkg": "./scripts/build-pkg.sh",
        "build:watch": "yarn run build --watch",
        "bump": "ts-scripts bump",
        "docs": "ts-scripts docs",
        "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
        "lint:fix": "yarn lint --fix && yarn sync",
        "setup": "yarn $YARN_SETUP_ARGS && yarn run build --force",
        "start": "node service.js",
        "start:dev": "yarn build && env NODE_ENV=development node service.js",
        "sync": "ts-scripts sync",
        "test": "ts-scripts test"
    },
    "resolutions": {
        "@turf/clean-coords": "^6.0.1",
        "@types/lodash": "^4.14.162",
        "debug": "^4.1.1",
        "ms": "^2.1.2",
        "typescript": "~4.0.2"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/bluebird": "^3.5.32",
        "@types/convict": "^5.2.1",
        "@types/elasticsearch": "^5.0.35",
        "@types/fs-extra": "^9.0.2",
        "@types/jest": "^26.0.14",
        "@types/lodash": "^4.14.164",
        "@types/node": "^14.14.0",
        "@types/uuid": "^8.3.0",
        "eslint": "^7.12.1",
        "jest": "^26.6.1",
        "jest-extended": "^0.11.5",
        "jest-watch-typeahead": "^0.6.0",
        "node-notifier": "^8.0.0",
        "ts-jest": "^26.4.0",
        "typescript": "~4.0.2"
    },
    "engines": {
        "node": ">=10.16.0",
        "yarn": ">=1.16.0"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "documentation": "https://terascope.github.io/teraslice",
    "terascope": {
        "root": true,
        "type": "monorepo",
        "target": "es2018",
        "tests": {
            "suites": {
                "e2e": [
                    "elasticsearch",
                    "kafka"
                ],
                "elasticsearch": [
                    "elasticsearch"
                ],
                "kafka": [
                    "kafka"
                ],
                "unit-a": [],
                "unit-b": []
            }
        },
        "docker": {
            "registries": [
                "terascope/teraslice"
            ]
        },
        "npm": {
            "registry": "https://registry.npmjs.org/"
        }
    }
}
