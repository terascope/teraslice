{
    "name": "teraslice-workspace",
    "version": "0.61.0",
    "private": true,
    "homepage": "https://github.com/terascope/teraslice",
    "bugs": {
        "url": "https://github.com/terascope/teraslice/issues"
    },
    "workspaces": [
        "packages/*",
        "e2e"
    ],
    "scripts": {
        "benchmark": "lerna run --concurrency=1 --no-prefix --stream benchmark",
        "prebuild": "lerna run prebuild",
        "build": "tsc --build",
        "build:all": "yarn build:cleanup && env FORCE_COLOR=1 yarn lerna run build",
        "build:cleanup": "./scripts/build-cleanup.sh",
        "build:fix": "./scripts/build-fix.sh",
        "build:pkg": "./scripts/build-pkg.sh",
        "build:watch": "yarn run build --watch",
        "bump": "ts-scripts bump",
        "cleanup": "yarn build:fix",
        "docs": "ts-scripts docs",
        "lint": "yarn lint:es && yarn sync --verify",
        "lint:es": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
        "lint:fix": "yarn lint:es --fix && yarn sync",
        "quick:setup": "yarn lerna link --force-local && yarn build",
        "setup": "yarn && yarn lerna link --force-local && yarn build:cleanup && yarn build && yarn sync -q",
        "start": "node service.js",
        "sync": "ts-scripts sync",
        "test": "ts-scripts test"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/bluebird": "^3.5.28",
        "@types/convict": "^4.2.1",
        "@types/elasticsearch": "^5.0.35",
        "@types/fs-extra": "^8.0.1",
        "@types/jest": "^24.0.23",
        "@types/lodash": "^4.14.149",
        "@types/nanoid": "^2.0.0",
        "@types/node": "^12.12.14",
        "@types/uuid": "^3.4.6",
        "eslint": "^6.7.2",
        "jest": "^24.9.0",
        "jest-extended": "^0.11.2",
        "jest-watch-typeahead": "^0.4.0",
        "lerna": "^3.19.0",
        "lerna-alias": "^3.0.2",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.2"
    },
    "resolutions": {
        "@types/lodash": "^4.14.149",
        "debug": "^4.1.1",
        "ms": "^2.1.2",
        "typescript": "^3.7.2"
    },
    "engines": {
        "node": ">=10.16.0",
        "yarn": ">=1.16.0"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "displayName": "Teraslice",
    "documentation": "https://terascope.github.io/teraslice",
    "terascope": {
        "root": true,
        "type": "monorepo",
        "tests": {
            "suites": {
                "e2e": [
                    "elasticsearch",
                    "kafka"
                ],
                "elasticsearch": [
                    "elasticsearch"
                ],
                "kafka": [
                    "kafka"
                ],
                "unit-a": [],
                "unit-b": []
            }
        },
        "docker": {
            "registries": [
                "terascope/teraslice"
            ]
        },
        "npm": {
            "registry": "https://registry.npmjs.org/"
        }
    }
}
