{
  "name": "teraslice",
  "version": "0.37.1",
  "description": "Slice and dice your Elasticsearch data",
  "bin": "service.js",
  "main": "service.js",
  "scripts": {
    "start": "node service.js",
    "lint": "eslint *.js lib/**/*.js",
    "lint:fix": "eslint --fix *.js lib/**/*.js",
    "test": "nyc -x spec/ --reporter=text-summary jasmine; nyc report --reporter=html",
    "integration-tests": "yarn --cwd ./integration-tests test",
    "report-coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov && codecov"
  },
  "files": [
    "*.js",
    "lib/**/*",
    "examples/**/*"
  ],
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "*.js",
      "lib/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terascope/teraslice.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/terascope/teraslice/issues"
  },
  "homepage": "https://github.com/terascope/teraslice#readme",
  "dependencies": {
    "@terascope/elasticsearch-api": "^1.0.0",
    "@terascope/error-parser": "^1.0.0",
    "@terascope/queue": "^1.1.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "convict": "^4.3.1",
    "datemath-parser": "^1.0.6",
    "decompress": "^4.2.0",
    "easy-table": "^1.1.1",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "json-templater": "^1.2.0",
    "kubernetes-client": "^5.3.0",
    "lodash": "^4.17.5",
    "mocker-data-generator": "^2.6.1",
    "moment": "^2.21.0",
    "node-cache": "^4.2.0",
    "request": "^2.85.0",
    "shortid": "^2.2.12",
    "socket.io": "~1.4.5",
    "socket.io-client": "~1.4.5",
    "terafoundation": "^0.2.1",
    "uuid": "^3.3.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@terascope/teraslice-op-test-harness": "^1.0.0",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-schema-faker": "^0.5.0-rc13",
    "nock": "^9.4.2",
    "nyc": "^12.0.2"
  }
}
